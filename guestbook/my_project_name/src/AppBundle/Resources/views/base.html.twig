
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}My Application{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/test.css') }}"/>
    {% endblock %}
</head>
<body>
{% block body %}
    <div id="sidebar">
        <a href="{{ path('homepage') }}">homepage</a>
        <a href="{{ path('feedback') }}">feedback</a>
        <a href="{{ path('products_list') }}">products</a>
        <a href="{{ path('categories') }}">categories</a>
    </div>

    {% for flash in app.session.flashBag.get('success1') %}
        {{ flash }}
    {% endfor %}
    <div id="content">
        {% block content %}{% endblock %}
    </div>
{% endblock %}
</body>
</html>